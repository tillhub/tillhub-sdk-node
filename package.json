{
  "name": "tillhub-sdk-node",
  "version": "0.0.1",
  "description": "Tillhub SDK for Node.js",
  "main": "./lib/index.js",
  "scripts": {
    "test": "tape 'test/**/*.spec.js'",
    "tdd": "NODE_ENV=test DEBUG=api:* nodemon --exec nodemon test/tdd-runner.js",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "precommit": "git add -A",
    "commit": "npm run precommit && git-cz",
    "preversion": "npm run lint && npm test",
    "postversion": "TAG=$(echo $(git describe --tags $(git rev-list --tags --max-count=1))) && git tag -a $TAG $TAG -f -m \"$(git log `git describe --tags --abbrev=0 HEAD^`..HEAD --oneline)\" && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tillhub/tillhub-sdk-node.git"
  },
  "keywords": [
    "tillhub",
    "sdk",
    "data"
  ],
  "author": "Robert Jefe Lindstaedt <robert.lindstaedt@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tillhub/tillhub-sdk-node/issues"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "homepage": "https://github.com/tillhub/tillhub-sdk-node#readme",
  "devDependencies": {
    "@types/node": "^10.0.3",
    "chalk": "^2.4.1",
    "codecov": "^3.0.1",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.22",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "glob": "^7.1.2",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "proxyquire": "^2.0.1",
    "semantic-release": "^15.2.0",
    "sinon": "^5.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0",
    "tape-nock": "^1.6.0",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^0.18.0",
    "is": "^3.2.1",
    "pump": "^3.0.0",
    "request": "^2.85.0",
    "retry-request": "^3.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  }
}
